{
  "name": "react-native-contains",
  "version": "0.1.11",
  "description": "Universal element contains for react, react-native, and react-native web",
  "keywords": [
    "react-native",
    "react-native-web",
    "react",
    "native",
    "web",
    "dom",
    "contains",
    "universal",
    "typescript"
  ],
  "repository": "kmalakoff/react-native-contains",
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "unpkg": "dist/umd/react-native-contains.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "coverage": "tsds coverage",
    "deploy": "tsds deploy",
    "format": "tsds format",
    "lint": "tsds lint",
    "test": "tsds test:node",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.4",
    "@types/react": "18.0.15",
    "@types/react-native": "0.69.3",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.3",
    "react-native-web": "0.18.4",
    "react-test-renderer": "18.0.0",
    "ts-dev-stack": "^0.8.6"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">=14"
  },
  "tsds": {
    "source": "src/index.ts",
    "globals": {
      "react": "React",
      "react-native": "ReactNative"
    }
  }
}
