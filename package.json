{
  "name": "react-native-contains",
  "version": "0.1.2",
  "description": "Universal element contains for react, react-native, and react-native web",
  "keywords": [
    "react-native",
    "react-native-web",
    "react",
    "native",
    "web",
    "contains",
    "universal",
    "typescript"
  ],
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./*": "./*"
  },
  "types": "dist/esm/index.d.ts",
  "repository": "kmalakoff/react-native-contains",
  "scripts": {
    "build": "tsds build",
    "deploy": "npm run format && npm run lint && npm run build && npm run test:engines && npm publish",
    "format": "tsds format",
    "lint": "tsds lint",
    "test": "tsds link && NODE_OPTIONS=--experimental-vm-modules BABEL_ENV=test jest test/**/*.test.*",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-native": "^0.68.0",
    "@types/react-test-renderer": "^18.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "metro-react-native-babel-preset": "^0.71.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.69.0",
    "react-test-renderer": "18.2.0",
    "ts-dev-stack": "^0.2.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">=12"
  },
  "unpkg": "dist/umd/react-native-contains.js"
}
